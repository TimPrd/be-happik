{
    "name": "node-docker",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": ">=11.3.0",
        "npm": ">=6.4.1"
    },
    "scripts": {
        "start": "node ./node_modules/.bin/nodemon -L bin/www",
        "test": "./node_modules/.bin/mocha",
        "cover": "./node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha test/index.js",
        "travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*",
        "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "db:reset": "docker stop bh_postgres && sleep 5 && docker rm bh_postgres && npm run db:install",
        "db:install": "docker-compose build && docker-compose up -d && sleep 10 && make && make db-install",
        "doc": "apidoc -i ./app -o ../docs/"
    },
    "dependencies": {
        "@sendgrid/mail": "^6.3.1",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "~1.18.2",
        "chai": "^4.2.0",
        "chai-http": "^4.2.0",
        "cookie-parser": "~1.4.3",
        "cors": "^2.8.5",
        "coveralls": "^3.0.2",
        "deep-extend": "0.6.0",
        "dotenv": "^6.2.0",
        "express": "~4.16.2",
        "faker": "^4.1.0",
        "hbs": "~4.0.1",
        "install": "^0.12.2",
        "istanbul": "^0.4.5",
        "jsonwebtoken": "^8.4.0",
        "lodash": "4.17.11",
        "mocha": "^5.2.0",
        "morgan": "~1.9.0",
        "nodemailer": "^4.7.0",
        "nodemon": "^1.18.9",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^6.4.2",
        "pg-hstore": "^2.3.2",
        "randomatic": "3.1.1",
        "sequelize": "~4.28.8",
        "sequelize-cli": "^5.4.0",
        "serve-favicon": "~2.4.5",
        "socket.io": "^2.4.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "istanbul": "0.3.5",
        "mocha": "2.0.0",
        "coveralls": "^3.0.2"
    }
}
